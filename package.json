{
  "name": "@softwareventures/decimal",
  "version": "0.0.0-development",
  "description": "High precision decimal numeric data type",
  "keywords": [
    "TypeScript",
    "decimal",
    "numeric"
  ],
  "author": "Daniel Cassidy <mail@danielcassidy.me.uk>",
  "homepage": "https://github.com/softwareventures/decimal",
  "bugs": "https://github.com/softwareventures/decimal/issues",
  "repository": "github:softwareventures/decimal",
  "license": "ISC",
  "scripts": {
    "fix": "tsc --noEmit && tslint --fix --project .",
    "lint": "tsc --noEmit && tslint --project .",
    "prepare": "tsc",
    "semantic-release": "semantic-release",
    "test": "tslint --project . && ava"
  },
  "sideEffects": false,
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "dependencies": {
    "@softwareventures/ordered": "^0.2.0 || ^1.0.0",
    "i32": "^0.3.0 || ^1.0.0"
  },
  "devDependencies": {
    "@softwareventures/semantic-release-config": "2.0.0",
    "@softwareventures/tsconfig": "5.1.1",
    "@softwareventures/tslint-rules": "1.0.0",
    "ava": "4.1.0",
    "cz-conventional-changelog": "3.3.0",
    "semantic-release": "17.4.7",
    "ts-node": "10.7.0",
    "tslint": "6.1.3",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "release": {
    "extends": "@softwareventures/semantic-release-config"
  }
}
